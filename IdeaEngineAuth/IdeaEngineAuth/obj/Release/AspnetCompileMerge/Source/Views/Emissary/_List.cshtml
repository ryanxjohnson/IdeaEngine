@model EmissaryOverviewList

<table id="emissary_table" class="table">
    <thead>
        <tr class="idea_engine_header_row">
            <th class="idea_engine_th" style="border-top-left-radius: 8px; width:120px;">Name</th>
            <th class="idea_engine_th" style="width:10%;">Domain ID</th>
            <th class="idea_engine_th" style="width:10%;">Admin</th>
            <th class="idea_engine_th" style="width:10%;">Ambassador</th>
            <th class="idea_engine_th" style="width:90px; text-align: center; border-top-right-radius: 8px;">Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
                <tr class="idea_engine_row" onclick="document.location = 'Emissary/Details/@item.ID'">
                    <td>@Html.DisplayFor(modelItem => item.Name)</td>
                    <td>@Html.DisplayFor(modelItem => item.Domain_ID)</td>
                    <td>@Html.ToYesNoString(item.Admin)</td>
                    <td>@Html.ToYesNoString(item.Ambassador)</td>
                    <td align="center">
                        @Html.Partial("_ListActions", item)
                    </td>
                </tr>
        }
    </tbody>
</table>